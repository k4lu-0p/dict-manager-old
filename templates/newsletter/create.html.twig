<div class="container container-newsletter">

    <h1>{{ title }}</h1>

    {{ form_start(form) }}

         {{ form_row(form.subject, {'required' : false, 'label' : false ,'attr' : {'class': 'form-control mb-3', 'placeholder' : 'Subject'}}) }}
         {{ form_row(form.content, {'required' : false, 'label' : false ,'attr' : {'class': 'form-control mb-3', 'placeholder' : 'Content'}}) }}
         {{ form_row(form.picture, {'required' : false, 'label' : false ,'attr' : {'class': 'form-control mb-3', 'placeholder' : 'Picture'}}) }}
         {{ form_row(form._token)}}
        <button id="send-button-newsletter" type="submit" class="button-newsletter-send">Send to all</button>


    {{ form_end(form) }}
</div>