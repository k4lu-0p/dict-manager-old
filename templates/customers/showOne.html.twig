<div class="container container-customer">

    <h1>{{ customer.createdAt | date("m/d/Y") }}</h1>
    <button id="button-previous">Previous</button>

    <div class="card">

        <h2>{{ customer.id }}</h2>
        <h2>{{ customer.firstname }}</h2>
        <h2>{{ customer.lastname }}</h2>
        <h2>{{ customer.phone }}</h2>
        <h2>{{ customer.email }}</h2>

        <h2>{{ customer.addressNumber }}</h2>
        <h2>{{ customer.streetNumber }}</h2>
        <h2>{{ customer.streetName }}</h2>
        <h2>{{ customer.city }}</h2>
        <h2>{{ customer.pc }}</h2>
        <h2>{{ customer.country }}</h2>
        <h2>{{ customer.building }}</h2> 
        
        <h2>{{ customer.picture }}</h2>
        <h2>{{ customer.newsletter }}</h2>
        

    </div>
</div>